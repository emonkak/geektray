{
    "ui": {
        "window_name": "KeyTray",
        "window_class": "KeyTray",
        "window_padding": 0,
        "window_width": 480,
        "item_gap": 0,
        "item_padding": 8.0,
        "icon_size": 24.0,
        "font": {
            "size": 12.0,
            "family": "Sans",
            "weight": "Normal",
            "style": "Normal",
            "stretch": "Normal"
        },
        "color": {
            "window_background": "#21272b",
            "normal_item_background": "#21272b",
            "normal_item_foreground": "#e8eaeb",
            "selected_item_background": "#1c95e6",
            "selected_item_foreground": "#e8eaeb"
        }
    },
    "keys": [
        {
            "key": "j",
            "commands": ["SelectNextItem"]
        },
        {
            "key": "Down",
            "commands": ["SelectNextItem"]
        },
        {
            "key": "n",
            "modifiers": {
                "control": true
            },
            "commands": ["SelectNextItem"]
        },
        {
            "key": "k",
            "commands": ["SelectPreviousItem"]
        },
        {
            "key": "Down",
            "commands": ["SelectPreviousItem"]
        },
        {
            "key": "p",
            "modifiers": {
                "control": true
            },
            "commands": ["SelectPreviousItem"]
        },
        {
            "key": "l",
            "commands": ["ClickLeftButton", "HideWindow"]
        },
        {
            "key": "Return",
            "commands": ["ClickLeftButton", "HideWindow"]
        },
        {
            "key": "h",
            "commands": ["ClickRightButton", "HideWindow"]
        },
        {
            "key": "Return",
            "modifiers": {
                "shift": true
            },
            "commands": ["ClickRightButton", "HideWindow"]
        },
        {
            "key": "q",
            "commands": ["HideWindow"]
        },
        {
            "key": "Escape",
            "commands": ["HideWindow"]
        }
    ],
    "print_x11_errors": true
}
